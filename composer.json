{
  "name": "dynamic/dynamic-blocks",
  "description": "A collection of blocks for the SilverStripe Blocks module.",
  "authors": [
    {
      "name": "Dynamic",
      "email": "dev@dynamicagency.com",
      "homepage": "http://www.dynamicagency.com"
    }
  ],
  "keywords": [
    "silverstripe"
  ],
  "type": "silverstripe-module",
  "license": "BSD-3-Clause",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:jsirish/silverstripe-flexslider.git"
    }
  ],
  "require": {
    "silverstripe/cms": "^4.0",
    "silverstripe/framework": "^4.0",
    "dynamic/flexslider": "^3.0",
    "sheadawson/silverstripe-blocks": "^2.0",
    "dynamic/silverstripe-linkable": "^1.0",
    "symbiote/silverstripe-gridfieldextensions": "^3.0",
    "dynamic/silverstripe-geocoder": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\DynamicBlocks\\": "src/",
      "Dynamic\\DynamicBlocks\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-name": "dynamic-blocks",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
